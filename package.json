{
	"name": "testme-server",
	"version": "1.0.0",
	"description": "TestMe server responsible for handling client requests, logging, fake auth and many more...",
	"main": "index.js",
	"scripts": {
		"start": "pm2 start config/pm2.json --env production",
		"dev": "pm2 start config/pm2.json",
		"stop": "pm2 stop TestMe-server",
		"reload": "pm2 reload TestMe-server",
		"restart": "pm2 restart TestMe-server",
		"delete": "pm2 delete TestMe-server",
		"log": "pm2 logs TestMe-server",
		"monit": "pm2 monit TestMe-server",
		"test": "NODE_ENV=test jest --watchAll --group=unit/models",
		"debug": "DEBUG=* npx nodemon index.js",
		"lint": "eslint --ext .js ./",
		"format": "prettier --write \"*.js\"",
		"version": "conventional-changelog -p emojis -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
		"audit": "npm audit fix --only=prod"
	},
	"keywords": [
		"Quiz",
		"Server",
		"Express"
	],
	"author": "Rbiha Louaie",
	"license": "ISC",
	"devDependencies": {
		"conventional-changelog-cli": "^2.1.0",
		"conventional-changelog-core": "^4.2.0",
		"conventional-changelog-emojis": "^3.1.0",
		"debug": "^4.2.0",
		"eslint": "^7.12.1",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-node": "^4.1.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-security": "^1.4.0",
		"jest": "^26.6.1",
		"jest-runner-groups": "^2.0.1",
		"morgan": "^1.10.0",
		"nodemon": "^2.0.6",
		"prettier": "^2.1.2",
		"supertest": "^5.0.0"
	},
	"dependencies": {
		"bcrypt": "^5.0.0",
		"config": "^3.3.2",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"express-mongo-sanitize": "^2.0.0",
		"express-rate-limit": "^5.1.3",
		"helmet": "^4.1.1",
		"hpp": "^0.2.3",
		"joi": "^17.3.0",
		"joi-objectid": "^3.0.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.10.11",
		"pm2": "^4.5.0",
		"winston": "^3.3.3",
		"xss-clean": "^0.1.1"
	}
}
