{
	"apps": [
		{
			"name": "TestMe-server",
			"script": "index.js",
			"watch": true,
			"ignore_watch": ["node_modules", "api/test", "api/public", "logs"],
			"watch_delay": 1000,
			"watch_options": {
				"followSymlinks": false
			},
			"env": {
				"PORT": 3000,
				"NODE_ENV": "development"
			},
			"env_production": {
				"PORT": 3000,
				"NODE_ENV": "production"
			}
		}
	]
}
